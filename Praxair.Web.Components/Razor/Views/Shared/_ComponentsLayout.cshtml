}


@section CL_StylesSection {

    <link rel="stylesheet" href="~/style/praxair.web.components.css">
    @RenderSection("L_StylesSection", false)

}

@section CL_ScriptsSection {

    @RenderSection("L_ScriptsSection", false)

}

@section CL_TopMenuSection {


}

@section CL_MenuBarSection {

